{
  "name": "@domeadev/react-markdown",
  "version": "0.0.2",
  "description": "A lightweight, customizable React component for rendering Markdown content with TypeScript support and extensible rendering.",
  "types": ".dist/index.d.ts",
  "main": ".dist/index.js",
  "module": ".dist/index.mjs",
  "files": [
    ".dist"
  ],
  "exports": {
    ".": {
      "import": "./.dist/index.mjs",
      "require": "./.dist/index.js"
    }
  },
  "keywords": [
    "react",
    "markdown",
    "typescript",
    "markdown-parser",
    "markdown-renderer",
    "react-component",
    "customizable",
    "lightweight",
    "extensible"
  ],
  "author": "domeafavour",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@storybook/addon-docs": "^9.1.10",
    "@storybook/addon-onboarding": "^9.1.10",
    "@storybook/react-vite": "^9.1.10",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/react": ">=16.8.0",
    "@types/react-dom": ">=16.8.0",
    "happy-dom": "^16.5.3",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "storybook": "^9.1.10",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "vitest": "^2.1.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/domeafavour/react-markdown"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "vitest",
    "build": "tsup",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build-storybook:ghpages": "storybook build --output-dir storybook-static"
  },
  "dependencies": {
    "@domeadev/react-elements-renderer": "^0.0.1",
    "marked": "^16.2.0"
  }
}
